   "source": [
    "n = int(input())\n",
    "s = set(map(int, input().split()))\n",
    "N = int(input())\n",
    "\n",
    "for i in range(N):\n",
    "    l = input().split()\n",
    "    if l[0]=='pop':\n",
    "        s.pop()\n",
    "    elif l[0]=='remove':\n",
    "        s.remove(int(l[1]))\n",
    "    elif l[0]=='discard':\n",
    "        s.discard(int(l[1]))\n",
    "print(sum(s))\n"
   ]
  }
